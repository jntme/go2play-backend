{
  "name": "Game",
  "plural": "games",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "user1": {
      "type": "P2GUser",
      "required": true
    },
    "user2": {
      "type": "P2GUser",
      "required": true
    },
    "activeRound": {
      "type": "Number",
      "required": false,
      "default": 1
    },
    "activeUser": {
      "type": "String",
      "required": false
    },
    "questionHashMap": {
      "type": "array",
      "required": false
    },
    "rounds": {
      "type": [
        "Object"
      ],
      "required": false
    }
  },
  "validations": [],
  "relations": {

    "user1" : {
      "type" : "belongsTo",
      "model" : "P2GUser",
      "foreignKey" : "user1"
    },

    "user2" : {
      "type" : "belongsTo",
      "model" : "P2GUser",
